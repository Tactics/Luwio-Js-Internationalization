{
  "name": "@luwio/routing",
  "version": "0.0.1",
  "description": "Router for all React application powered by Luwio",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "require": "./build/index.js",
      "import": "./build/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prettify": "pnpm exec prettier --write src/.",
    "build-github": "pnpm run prettify && pnpm exec tsc --noEmit && pnpm run build",
    "build": "pnpm run prettify && pnpm exec tsup",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "react": ">= 19"
  },
  "dependencies": {
    "react": "^19",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
